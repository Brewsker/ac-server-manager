# Server Configuration
PORT=3001
NODE_ENV=development

# Assetto Corsa Paths
# Path to AC server executable (e.g., C:/Steam/steamapps/common/assettocorsa/server/acServer.exe)
AC_SERVER_PATH=

# Path to server_cfg.ini (e.g., C:/Steam/steamapps/common/assettocorsa/server/cfg/server_cfg.ini)
AC_SERVER_CONFIG_PATH=

# Path to entry_list.ini (e.g., C:/Steam/steamapps/common/assettocorsa/server/cfg/entry_list.ini)
AC_ENTRY_LIST_PATH=

# Path to AC content folder (e.g., C:/Steam/steamapps/common/assettocorsa/content)
AC_CONTENT_PATH=

# Database (SQLite)
DB_PATH=./data/acserver.db

# CORS Settings
CORS_ORIGIN=http://localhost:5173

# WebSocket
WS_PORT=3002
