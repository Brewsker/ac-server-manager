# AC Server Manager Containers
Host 192.168.1.71 ac-server
    HostName 192.168.1.71
    User root
    StrictHostKeyChecking accept-new
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_ed25519
    IdentityFile ~/.ssh/id_rsa

Host 192.168.1.70 git-cache
    HostName 192.168.1.70
    User root
    StrictHostKeyChecking accept-new
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_ed25519
    IdentityFile ~/.ssh/id_rsa
